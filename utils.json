{
    "webdriver_opts": [
        "--headless=new",
        "disable-infobars",
        "--disable-extensions",
        "--disable-gpu",
        "--disable-dev-shm-usage",
        "--no-sandbox"
    ],
    "elements": {
        "search_url": "https://kcnawatch.org/kctv-archive/?start=DATE&end=DATE",
        "search_date_pattern": "dd-MM-uuuu",
        "1_class": "article-desc",
        "2_class": "broadcast-head",
        "2_element": "Full Broadcast",
        "2_date_pattern": "EEEE LLLL dd, uuuu",
        "2_attribute": "href",
        "3_xpath": "//video[@id = 'bitmovinplayer-video-player']",
        "4_xpath": "source",
        "4_attribute": "src"
    }
}